    "import csv\n",
    "import os\n",
    "from datetime import datetime, timedelta\n",
    "from googleapiclient.discovery import build\n",
    "\n",
    "# --- ENHANCED CONFIGURATION WITH TIME FILTERING ---\n",
    "API_KEY = \"AIzaSyBt7Jb3_7neD9xbp63LSGv-_AXXN3cqvnU\"  # Your API key\n",
    "SEARCH_QUERY = \"maggi india\"  # Search term for videos\n",
    "MAX_VIDEOS = 300  # Maximum number of videos to fetch\n",
    "OUTPUT_VIDEOS_FILE = \"videos_filtered.csv\"\n",
    "OUTPUT_COMMENTS_FILE = \"comments_filtered.csv\"\n",
    "\n",
    "# TIME PERIOD FILTERING OPTIONS\n",
    "# Option 1: Use predefined time periods\n",
    "# TIME_PERIOD = \"last_month\"  # Options: \"last_week\", \"last_month\", \"last_3_months\", \"last_6_months\", \"last_year\"\n",
    "\n",
    "# Option 2: Use custom date range (comment out TIME_PERIOD above and uncomment below)\n",
    "START_DATE = \"2024-08-30\"  # Format: YYYY-MM-DD\n",
    "END_DATE = \"2025-08-30\"    # Format: YYYY-MM-DD\n",
    "\n",
    "def get_date_range(period=None, start_date=None, end_date=None):\n",
    "    \"\"\"\n",
    "    Convert time period or custom dates to YouTube API format (RFC 3339)\n",
    "    \"\"\"\n",
    "    now = datetime.utcnow()\n",
    "    \n",
    "    if start_date and end_date:\n",
    "        # Custom date range\n",
    "        start = datetime.strptime(start_date, \"%Y-%m-%d\")\n",
    "        end = datetime.strptime(end_date, \"%Y-%m-%d\")\n",
    "        return start.isoformat() + 'Z', end.isoformat() + 'Z'\n",
    "    \n",
    "    elif period:\n",
    "        # Predefined periods\n",
    "        if period == \"last_week\":\n",
    "            start = now - timedelta(weeks=1)\n",
    "        elif period == \"last_month\":\n",
    "            start = now - timedelta(days=30)\n",
    "        elif period == \"last_3_months\":\n",
    "            start = now - timedelta(days=90)\n",
    "        elif period == \"last_6_months\":\n",
    "            start = now - timedelta(days=180)\n",
    "        elif period == \"last_year\":\n",
    "            start = now - timedelta(days=365)\n",
    "        else:\n",
    "            print(f\"Unknown period: {period}. Using last month.\")\n",
    "            start = now - timedelta(days=30)\n",
    "        \n",
    "        return start.isoformat() + 'Z', now.isoformat() + 'Z'\n",
    "    \n",
    "    else:\n",
    "        # Default to last month\n",
    "        start = now - timedelta(days=30)\n",
    "        return start.isoformat() + 'Z', now.isoformat() + 'Z'\n",
    "\n",
    "def search_videos_by_time_period(api_key, query, max_videos=300, time_period=None, start_date=None, end_date=None):\n",
    "    \"\"\"\n",
    "    Search for YouTube videos within a specific time period\n",
    "    \"\"\"\n",
    "    youtube = build('youtube', 'v3', developerKey=api_key)\n",
    "    \n",
    "    # Get date range\n",
    "    published_after, published_before = get_date_range(time_period, start_date, end_date)\n",
    "    \n",
    "    print(f\"Searching for videos published between {published_after} and {published_before}\")\n",
    "    print(f\"Search query: '{query}'\")\n",
    "    \n",
    "    videos = []\n",
    "    next_page_token = None\n",
    "    \n",
    "    while len(videos) < max_videos:\n",
    "        try:\n",
    "            # Search for videos\n",
    "            search_request = youtube.search().list(\n",
    "                part=\"snippet\",\n",
    "                q=query,\n",
    "                type=\"video\",\n",
    "                publishedAfter=published_after,\n",
    "                publishedBefore=published_before,\n",
    "                maxResults=min(50, max_videos - len(videos)),\n",
    "                pageToken=next_page_token,\n",
    "                order=\"relevance\"  # You can change to \"date\", \"rating\", \"viewCount\", \"title\"\n",
    "            )\n",
    "            \n",
    "            search_response = search_request.execute()\n",
    "            \n",
    "            # Extract video information\n",
    "            for item in search_response['items']:\n",
    "                video_data = {\n",
    "                    'video_id': item['id']['videoId'],\n",
    "                    'title': item['snippet']['title'],\n",
    "                    'channel_title': item['snippet']['channelTitle'],\n",
    "                    'channel_id': item['snippet']['channelId'],\n",
    "                    'published_at': item['snippet']['publishedAt'],\n",
    "                    'description': item['snippet']['description'][:500] + '...' if len(item['snippet']['description']) > 500 else item['snippet']['description']\n",
    "                }\n",
    "                videos.append(video_data)\n",
    "            \n",
    "            # Check for next page\n",
    "            next_page_token = search_response.get('nextPageToken')\n",
    "            if not next_page_token:\n",
    "                break\n",
    "                \n",
    "        except Exception as e:\n",
    "            print(f\"Error searching videos: {e}\")\n",
    "            break\n",
    "    \n",
    "    print(f\"Found {len(videos)} videos\")\n",
    "    return videos\n",
    "\n",
    "def get_video_statistics(api_key, video_ids):\n",
    "    \"\"\"\n",
    "    Get detailed statistics for a list of video IDs\n",
    "    \"\"\"\n",
    "    youtube = build('youtube', 'v3', developerKey=api_key)\n",
    "    \n",
    "    # YouTube API allows max 50 video IDs per request\n",
    "    stats = {}\n",
    "    \n",
    "    for i in range(0, len(video_ids), 50):\n",
    "        batch_ids = video_ids[i:i+50]\n",
    "        \n",
    "        try:\n",
    "            stats_request = youtube.videos().list(\n",
    "                part=\"statistics,contentDetails\",\n",
    "                id=','.join(batch_ids)\n",
    "            )\n",
    "            \n",
    "            stats_response = stats_request.execute()\n",
    "            \n",
    "            for item in stats_response['items']:\n",
    "                video_id = item['id']\n",
    "                stats[video_id] = {\n",
    "                    'view_count': item['statistics'].get('viewCount', 0),\n",
    "                    'like_count': item['statistics'].get('likeCount', 0),\n",
    "                    'comment_count': item['statistics'].get('commentCount', 0),\n",
    "                    'duration': item['contentDetails'].get('duration', 'PT0S')\n",
    "                }\n",
    "                \n",
    "        except Exception as e:\n",
    "            print(f\"Error getting video statistics: {e}\")\n",
    "    \n",
    "    return stats\n",
    "\n",
    "def get_video_comments_enhanced(api_key, video_id, max_comments=1000):\n",
    "    \"\"\"\n",
    "    Enhanced version to fetch comments with better error handling\n",
    "    \"\"\"\n",
    "    youtube = build('youtube', 'v3', developerKey=api_key)\n",
    "    comments = []\n",
    "    \n",
    "    try:\n",
    "        request = youtube.commentThreads().list(\n",
    "            part=\"snippet\",\n",
    "            videoId=video_id,\n",
    "            maxResults=100,\n",
    "            textFormat=\"plainText\"\n",
    "        )\n",
    "        \n",
    "        comment_count = 0\n",
    "        while request and comment_count < max_comments:\n",
    "            response = request.execute()\n",
    "            \n",
    "            for item in response['items']:\n",
    "                if comment_count >= max_comments:\n",
    "                    break\n",
    "                    \n",
    "                comment = item['snippet']['topLevelComment']['snippet']\n",
    "                comments.append({\n",
    "                    'video_id': video_id,\n",
    "                    'author': comment['authorDisplayName'],\n",
    "                    'text': comment['textDisplay'],\n",
    "                    'published_at': comment['publishedAt'],\n",
    "                    'like_count': comment.get('likeCount', 0)\n",
    "                })\n",
    "                comment_count += 1\n",
    "            \n",
    "            if comment_count < max_comments:\n",
    "                request = youtube.commentThreads().list_next(request, response)\n",
    "            else:\n",
    "                break\n",
    "                \n",
    "    except Exception as e:\n",
    "        print(f\"Error fetching comments for video {video_id}: {e}\")\n",
    "    \n",
    "    return comments\n",
    "\n",
    "def save_videos_to_csv(videos, filename):\n",
    "    \"\"\"Save video data to CSV\"\"\"\n",
    "    if not videos:\n",
    "        print(\"No videos to save.\")\n",
    "        return\n",
    "    \n",
    "    headers = ['video_id', 'title', 'channel_title', 'channel_id', 'published_at', 'description', \n",
    "               'view_count', 'like_count', 'comment_count', 'duration']\n",
    "    \n",
    "    try:\n",
    "        with open(filename, 'w', newline='', encoding='utf-8') as csvfile:\n",
    "            writer = csv.DictWriter(csvfile, fieldnames=headers)\n",
    "            writer.writeheader()\n",
    "            writer.writerows(videos)\n",
    "        print(f\"Videos saved to {filename}\")\n",
    "    except Exception as e:\n",
    "        print(f\"Error saving videos to CSV: {e}\")\n",
    "\n",
    "def save_comments_to_csv_enhanced(comments, filename):\n",
    "    \"\"\"Enhanced CSV saving for comments\"\"\"\n",
    "    if not comments:\n",
    "        print(\"No comments to save.\")\n",
    "        return\n",
    "    \n",
    "    headers = ['video_id', 'author', 'published_at', 'like_count', 'text']\n",
    "    \n",
    "    try:\n",
    "        with open(filename, 'w', newline='', encoding='utf-8') as csvfile:\n",
    "            writer = csv.DictWriter(csvfile, fieldnames=headers)\n",
    "            writer.writeheader()\n",
    "            writer.writerows(comments)\n",
    "        print(f\"Comments saved to {filename}\")\n",
    "    except Exception as e:\n",
    "        print(f\"Error saving comments to CSV: {e}\")\n",
    "\n",
    "# --- MAIN EXECUTION WITH TIME FILTERING ---\n",
    "print(\"🚀 Starting YouTube video search with time period filtering...\")\n",
    "\n",
    "# Check if using predefined period or custom dates\n",
    "if 'TIME_PERIOD' in locals():\n",
    "    videos = search_videos_by_time_period(API_KEY, SEARCH_QUERY, MAX_VIDEOS, time_period=TIME_PERIOD)\n",
    "elif 'START_DATE' in locals() and 'END_DATE' in locals():\n",
