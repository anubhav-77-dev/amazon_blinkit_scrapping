    "import csv\n",
    "from googleapiclient.discovery import build\n",
    "\n",
    "# --- CONFIGURATION ---\n",
    "# IMPORTANT: Paste your API key and the YouTube video ID here\n",
    "API_KEY = \""  # 👈 Paste your API key here\n",
    "VIDEO_ID = \"MVkXT4byMk8\" # 👈 Paste the video ID here\n",
    "OUTPUT_FILENAME = \"comments2.csv\" # Name of the file to save comments\n",
    "\n",
    "def get_video_comments(api_key, video_id):\n",
    "    \"\"\"\n",
    "    Fetches all top-level comments from a YouTube video using the YouTube Data API.\n",
    "    \"\"\"\n",
    "    # Create a YouTube API service object\n",
    "    youtube = build('youtube', 'v3', developerKey=api_key)\n",
    "\n",
    "    # List to store all the comments\n",
    "    all_comments = []\n",
    "    \n",
    "    print(f\"Fetching comments for video ID: {video_id}\")\n",
    "\n",
    "    try:\n",
    "        # Initial request to get the first page of comments\n",
    "        request = youtube.commentThreads().list(\n",
    "            part=\"snippet\",\n",
    "            videoId=video_id,\n",
    "            maxResults=100,  # Max results per page is 100\n",
    "            textFormat=\"plainText\"\n",
    "        )\n",
    "        \n",
    "        # Loop through all pages of comments\n",
    "        while request:\n",
    "            response = request.execute()\n",
    "            \n",
    "            # Extract comments from the response\n",
    "            for item in response['items']:\n",
    "                comment = item['snippet']['topLevelComment']['snippet']\n",
    "                author = comment['authorDisplayName']\n",
    "                text = comment['textDisplay']\n",
    "                published_at = comment['publishedAt']\n",
    "                \n",
    "                all_comments.append({\n",
    "                    'author': author,\n",
    "                    'text': text,\n",
    "                    'published_at': published_at\n",
    "                })\n",
    "\n",
    "            # Check if there is a next page\n",
    "            request = youtube.commentThreads().list_next(request, response)\n",
    "            \n",
    "        print(f\"Successfully fetched {len(all_comments)} comments.\")\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred: {e}\")\n",
    "        return None\n",
    "\n",
    "    return all_comments\n",
    "\n",
    "def save_comments_to_csv(comments, filename):\n",
    "    \"\"\"\n",
    "    Saves a list of comments to a CSV file.\n",
    "    \"\"\"\n",
    "    if not comments:\n",
    "        print(\"No comments to save.\")\n",
    "        return\n",
    "        \n",
    "    # Define the headers for the CSV file\n",
    "    headers = ['author', 'published_at', 'text']\n",
    "    \n",
    "    try:\n",
    "        with open(filename, 'w', newline='', encoding='utf-8') as csvfile:\n",
    "            writer = csv.DictWriter(csvfile, fieldnames=headers)\n",
    "            writer.writeheader()\n",
    "            writer.writerows(comments)\n",
    "        print(f\"Comments saved to {filename}\")\n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred while saving to CSV: {e}\")\n",
    "\n",
    "# --- MAIN EXECUTION ---\n",
    "if __name__ == \"__main__\":\n",
    "    # 1. Replace the placeholders with your actual API key and Video ID\n",
    "    if API_KEY == \"YOUR_API_KEY\" or VIDEO_ID == \"YOUR_VIDEO_ID\":\n",
    "        print(\"🚨 Please update the API_KEY and VIDEO_ID variables in the script!\")\n",
    "    else:\n",
    "        # 2. Fetch the comments\n",
    "        video_comments = get_video_comments(API_KEY, VIDEO_ID)\n",
    "        \n",
    "        # 3. Save them to a CSV file\n",
    "        if video_comments:\n",
    "            save_comments_to_csv(video_comments, OUTPUT_FILENAME)"
   ]
